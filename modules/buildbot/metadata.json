{
  "name": "rji-buildbot",
  "version": "1.0.0",
  "author": "rji",
  "summary": "Manage a Buildbot CI installation with Puppet.",
  "license": "Apache-2.0",
  "source": "https://github.com/rji/puppet-buildbot",
  "project_page": "https://github.com/rji/puppet-buildbot",
  "issues_url": "https://github.com/rji/puppet-buildbot/issues",
  "tags": [
